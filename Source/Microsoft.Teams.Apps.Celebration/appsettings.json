{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "[TenantId]",
    "ClientId": "[ClientId]",
    "ApplicationIdURI": "[ApplicationIdURI]",
    "ValidIssuers": "https://login.microsoftonline.com/TENANT_ID/v2.0,https://sts.windows.net/TENANT_ID/"
  },
  "Logging": {
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Trace",
        "Microsoft": "Error"
      }
    },
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Bot": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MicrosoftAppId": "[MicrosoftAppId]",
  "MicrosoftAppPassword": "[MicrosoftAppPassword]",
  "ServiceBusConnection": "[ServiceBusConnection]",
  "StorageAccountConnectionString": "[StorageAccountConnectionString]",
  "DisableAuthentication": "true",
  "DisableTenantFilter": "true",
  "AllowedTenants": "[AllowedTenants]",
  "ApplicationInsights": {
    "InstrumentationKey": "[InstrumentationKey]"
  },

  "RetryCount": 3,
  "RetryCountOnThrottling": 100,
  "RetryDelay": 3, // Seconds
  "RetryDelayOnThrottling": 3, // Seconds

  "BaseUrl": "https://[Celebrations app base Rrl]",
  "ManifestAppId": "[Manifest app id]"
}
